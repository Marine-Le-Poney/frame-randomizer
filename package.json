{
  "name": "nuxt-app",
  "version": "0.1.1",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "repository": "github:steadygaze/frame-randomizer",
  "scripts": {
    "build": "nuxt build",
    "check": "npm run format && npm run lint && npm run vue-tsc",
    "cm": "cz",
    "coverage": "vitest run --coverage.enabled --coverage.reporter=text --coverage.reporter=lcov",
    "dev": "nuxt dev",
    "format": "prettier --write --ignore-unknown .",
    "formatcheck": "prettier --check --ignore-unknown .",
    "generate": "nuxt generate",
    "lint": "eslint --fix --ext .js,.ts,.vue .",
    "postinstall": "nuxt prepare",
    "precommit": "npm run formatcheck && npm run lint && npm run vue-tsc && npm run test && nuxt build pcdist",
    "prepare": "husky install",
    "preview": "nuxt preview",
    "test": "vitest run",
    "testw": "vitest watch",
    "tsc": "tsc --noEmit",
    "vue-tsc": "vue-tsc"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.6.7",
    "@nuxt/devtools": "^0.7.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/lodash.difference": "^4.5.7",
    "@types/lodash.intersection": "^4.4.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.once": "^4.1.7",
    "@types/shell-escape": "^0.2.1",
    "@types/uuid": "^9.0.2",
    "@vitest/coverage-v8": "^0.33.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.16.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "nuxt": "^3.6.5",
    "prettier": "^2.8.8",
    "prettier-plugin-sort-json": "^1.0.0",
    "vitest": "^0.33.0",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "@nuxtjs/i18n": "^8.0.0-beta.13",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@pinia/nuxt": "^0.4.11",
    "chalk": "^5.3.0",
    "detect-browser": "^5.3.0",
    "fuse.js": "^6.6.2",
    "glob": "^10.3.3",
    "grapheme-splitter": "^1.0.4",
    "json-stringify-deterministic": "^1.0.10",
    "lodash.difference": "^4.5.0",
    "lodash.intersection": "^4.4.0",
    "lodash.merge": "^4.6.2",
    "lodash.once": "^4.1.1",
    "nuxt-security": "^0.14.3",
    "p-limit": "^4.0.0",
    "pinia": "^2.1.6",
    "shell-escape": "^0.2.0",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown ."
    ],
    "*.test.{js,ts}": "vitest run",
    "*.{js,ts}": "eslint --fix"
  },
  "overrides": {
    "vue": "^3.2.47"
  }
}
